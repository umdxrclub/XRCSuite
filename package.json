    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.18",
    "@types/node-schedule": "^2.1.0",
    "@types/passport": "^1.0.11",
    "@types/react-color": "^3.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/tough-cookie": "^4.0.1",
    "@types/tough-cookie-file-store": "^2.0.1",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.4",
    "jest": "^28.1.3",
