{
  "name": "backend",
  "version": "0.1.0",
  "description": "A backend service that helps manage members, keep track of devices, provide lab status, etc.",
  "scripts": {
    "test": "jest",
    "start": "ts-node ./src/run.ts --transpile-only",
    "deploy": "ts-node ./src/deploy.ts",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon ./src/run.ts",
    "types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/run.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@payloadcms/bundler-webpack": "^1.0.5",
    "@payloadcms/db-mongodb": "^1.1.1",
    "@payloadcms/richtext-lexical": "^0.5.0",
    "axios": "^0.26.0",
    "axios-cookiejar-support": "^2.0.4",
    "canvas": "^2.11.2",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "discord.js": "^14.13.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "html5-qrcode": "^2.3.4",
    "http-cookie-agent": "^5.0.2",
    "json-rpc-2.0": "^1.4.1",
    "moment": "^2.29.4",
    "node-html-parser": "^5.4.2",
    "node-schedule": "^2.1.1",
    "node-ssh": "^13.0.0",
    "npm": "^8.19.2",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "payload": "^2.5.0",
    "pg": "^8.7.3",
    "react-color": "^2.19.3",
    "sequelize": "^6.16.1",
    "sharp": "^0.33.1",
    "tough-cookie": "^4.0.0",
    "tough-cookie-file-store": "^2.0.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.5.0",
    "util": "^0.12.5",
    "uuid": "^9.0.0",
    "vrchat": "^1.13.0",
    "ws": "^8.12.0",
    "zod": "^3.21.4"
  },
